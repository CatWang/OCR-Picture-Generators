seed = len(letters)

print seed

with codecs.open('with-med/30w-piaoju-with-med.csv', 'w', encoding='utf-8') as csvfile:
    hh = 0
    while (hh < 10000):
        hh = hh + 1

        rootDir = '/alter/Piaoju-background/'
        list_dir = os.walk(rootDir)

        for root, d, fs in list_dir:
            for file in fs:
                image_path = os.path.join(root, file)
                print(image_path)
                im = Image.open(image_path)
                width, height = im.size
                print im.size
                print width
                print height
                print im.mode
                draw = ImageDraw.Draw(im)

                length = random.randint(1, 30)

                chineseLength = length / 3
                remainder = length % 3

                if (remainder > 0):
                    length = chineseLength + 1
                else:
                    length = chineseLength

                fontsize = random.randint(20, 40)

                remainderY = height - fontsize - 15
                if (remainderY <= 0):
                    fontsize = height - 13
                    remainderY = 3

                remainderX = width - length * (fontsize + 15)
                if (remainderX <= 0):
                    length = int(width / (fontsize + 15)) - 1
                    if (length <= 0):
                        continue
                    remainderX = width - length * (fontsize + 15)
                    if (remainderX <= 5):
                        remainderX = 6

                ll = length;

                string = ''
                font = random.randint(1, fonts)
                font_select = 1
                print font

                fontDir = "/alter/fonts"
                font_dir = os.walk(fontDir)

                path = ""

                for roo, dirs, files in font_dir:
                    print 1
                    for f in files:
                        if(font_select == font):
                            print font
                            path = os.path.join(roo, f)
                            print path
                            break
                        font_select = font_select + 1

                ttfont = ImageFont.truetype(path, fontsize)

                print path

                while (length > 0):
                    choice = random.randint(0, seed - 1)
                    print letters[choice]
                    string = string + letters[choice]
                    length = length - len(letters[choice])

                # r = random.randint(0, 255)
                # g = random.randint(0, 255)
                # b = random.randint(0, 255)

                # watermark = Image.new("RGBA", im.size)
                # waterdraw = ImageDraw.ImageDraw(watermark, "RGBA")
                try:
                    radX = random.randint(0, remainderX - 4)
                except:
                    continue
                try:
                    radY = random.randint(0, remainderY - 2)
                except:
                    continue

                draw.text((4 +  radX, 2 + radY), string.decode('utf-8'), fill=(105, 105, 105), font=ttfont)
                t_width, t_height = draw.textsize(unicode(string, 'UTF-8'), font = ttfont)
                # print r
                # print g
                # print b

                print string

                #box = (0, 0, 20 + t_width, 30 + t_height)

                rX = 8 + radX + t_width
                rY = 12 + radY + t_height

                box = (radX, radY, rX, rY)

                alpha = random.randint(20, 160)

                # watermask = watermark.convert("L").point(lambda x: min(x, alpha))
                #
                # watermark.putalpha(watermask)
                #
                # im.paste(watermark, None, watermark)

                region = im.crop(box)

                index = index + 1

                filename = 'with-med/30w-piaoju-with-med/' + str(index) + '.jpg'

                fname = str(index) + '.jpg'

                region.save(filename)

                csvfile.write('%s %s\n' % (fname, string.decode('utf-8')))
